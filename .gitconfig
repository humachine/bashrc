[alias]
        co = checkout
        cm = commit -m
        st = status
        br = branch
        lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
        ca = commit --amend

[color]
        ui = auto
[core]
        editor = vim
	excludesfile = /home/morpheus/.gitignore_global

[push]
	default = simple
